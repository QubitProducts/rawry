{
  "name": "rawry",
  "version": "1.0.0",
  "description": "ESWhatever sugar for nicely and safely executing raw sql",
  "main": "dist/rawry.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "test-unit": "mocha",
    "test": "npm run lint && npm run test-unit",
    "clean": "rm -rf dist",
    "compile": "babel src -d dist",
    "build": "npm run clean && npm run compile",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "before",
      "after"
    ],
    "ignore": [
      "dist/*"
    ]
  },
  "keywords": [
    "sql",
    "mysql"
  ],
  "author": "David Padbury <david@davidpadbury.com>",
  "license": "MIT",
  "dependencies": {
    "mysql": "^2.11.1",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "standard": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-register": "^6.9.0"
  }
}
